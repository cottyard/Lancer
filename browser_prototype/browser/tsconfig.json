{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "outDir": "../target/script",
        "rootDir": "..",
        "removeComments": true,
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "strict": true
    }
}
